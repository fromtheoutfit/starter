# Q: What?
# A: http://EditorConfig.org

# This is the top-most EditorConfig file. See http://editorconfig.org/#file-location
# for more information.
root = true

# All files.
[*]
end_of_line = lf
indent_style = space
indent_size = 2
charset = utf-8
insert_final_newline = true
trim_trailing_whitespace = true

# Markdown files.
[*.md]
trim_trailing_whitespace = false
