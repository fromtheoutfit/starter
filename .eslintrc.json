{
    "extends": "eslint:recommended",
    "parserOptions": {
      "sourceType": "module"
    },
    "env": {
        "browser": true,
        "es6": true,
        "jquery": true,
        "node": true,
        "serviceworker": true
    },
    "globals": {
        "ally": false
    },
    "rules": {
        "eqeqeq": ["error", "always"],
        "indent": ["warn", 2],
        "quotes": ["error", "single"],
        "semi": ["warn", "never"]
    }
}
